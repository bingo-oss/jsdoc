{
    "plugins": ["plugins/markdown"],
    "markdown": {
        "parser": "gfm",
        "hardwrap": true
    },
    "templates": {
        "name": "WEEX JSSDK",
        "logo": {
            "link": "http://dev.bingocc.com/linkapi/v0.3.10/index.html"
        },
        "version": "0.0.1",
        "css": [
            "../styles/extrand.css"
        ]
    },
    "source":{
        "include": [ "../mobile-linkjsapi/linkapi.js" ],  //需要生成文档的js入口文件
        "includePattern": ".+\\.js(doc)?$"
    },
    "opts": {
        "encoding": "utf8",
        "recurse": true,
        "destination": "./out/weex/",  //输出文件地址
        "readme": "./index-md/weex.md"   //js的首页说明文档
    }
}
