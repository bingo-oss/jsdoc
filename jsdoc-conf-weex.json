{
    "plugins": ["plugins/markdown"],
    "markdown": {
        "parser": "gfm",
        "hardwrap": true
    },
    "templates": {
        "name": "WEEX JSSDK",
        "logo": {
            "link": "http://dev.bingocc.com/linkapi/v0.3.10/index.html"
        },
        "version": "0.0.1",
        "css": [
            "../styles/extrand.css"
        ]
    },
    "source":{
        "include": [ "../mobile-linkjsapi/linkapi.js" ],
        "includePattern": ".+\\.js(doc)?$",
    },
    "opts": {
        "encoding": "utf8",
        "recurse": true,
        "destination": "./out-weex/",
        "readme": "./weex.md"
    }
}
